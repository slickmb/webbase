<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <clear/>
                <add value="index.html" />
            </files>
        </defaultDocument>
    </system.webServer>

    <location path="img">
        <system.webServer>
            <staticContent>
                <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365:00:00:00" />
            </staticContent>
        </system.webServer>
    </location>

    <location path="js">
        <system.webServer>
            <staticContent>
                <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365:00:00:00" />
            </staticContent>
        </system.webServer>
    </location>

    <location path="css">
        <system.webServer>
            <staticContent>
                <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365:00:00:00" />
            </staticContent>
        </system.webServer>
    </location>
</configuration>
